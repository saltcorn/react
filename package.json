{
  "name": "@saltcorn/react",
  "version": "0.0.1",
  "description": "integrate React components in your Saltcorn application",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production --config build-setup/webpack.config.js",
    "builddev": "webpack --mode development --config build-setup/webpack.config.js"
  },
  "dependencies": {
    "@saltcorn/markup": "^0.2.0",
    "@saltcorn/data": "^0.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1",
    "babel-loader": "^10.0.0",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3"
  },
  "author": "Christian Hugo",
  "license": "MIT",
  "repository": "github:saltcorn/react",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": "off",
      "no-case-declarations": "off",
      "no-empty": "warn",
      "no-fallthrough": "warn"
    }
  }
}
